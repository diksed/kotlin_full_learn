        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kotlin_full_learn"
        tools:targetApi="31">
        <activity
            android:name=".landmark_book.DetailsActivity"
